{
    "name": "下載影片",
    "description": "練習練習練習練習練習練習練習",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],
    "action": {
      "default_title": "TITLE",
      "default_popup": "test.html",
      "default_icon": "cat.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }